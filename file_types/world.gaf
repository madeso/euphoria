package world;

message vec2f
{
  optional float x = 1;
  optional float y = 2;
}

message Property
{
  optional string name = 1;
  // only one is valid
  optional vec2f vec2f = 100;
}

message Object
{
  optional string template = 1;
  repeated Property properties = 2;
}

message World
{
  repeated Object objects = 1;
}
